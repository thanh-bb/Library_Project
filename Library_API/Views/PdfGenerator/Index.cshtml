@model IEnumerable<Library_API.Models.PhieuDongPhat>

<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>PhieuDongPhat List</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body>
    <div class="container">
        <h1>PhieuDongPhat List</h1>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">TongTienPhat</th>
                    <th scope="col">NgayDong</th>
                    <th scope="col">TrangThaiDong</th>
                    <th scope="col">PmId</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var phieuDongPhat in Model)
                {
                    <tr>
                        <th scope="row">@phieuDongPhat.PdpId</th>
                        <td>@phieuDongPhat.PdpTongTienPhat</td>
                        <td>@phieuDongPhat.PdpNgayDong</td>
                        <td>@phieuDongPhat.PdpTrangThaiDong</td>
                        <td>@phieuDongPhat.PmId</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>
</html>
